package sort;

public class BubbleSort {
	
	
	public static void PrintFunn()
	{
		
		
	}

	
	public static void main(String [] args) {
		
		int [] arr= {9,13,25,5,6,32,1,4,8,7};
		
		
		for(int i=0;i<arr.length-1;i++) {
			for(int j=0;j<arr.length-1;j++) {
				
				if(arr[i]>arr[i+1])
				{
					int temp=arr[i];
					arr[i]=arr[i+1];
					arr[i+1]=temp;
				}
			}
			
			
		}
	}
	                                        
	                                        
	                                        
}
